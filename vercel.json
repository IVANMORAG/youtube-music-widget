{
  "version": 2,
  "builds": [
    {
      "src": "api/now-playing.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/now-playing",
      "dest": "/api/now-playing"
    },
    {
      "src": "/",
      "dest": "/api/now-playing",
      "headers": {
        "Cache-Control": "s-maxage=60, stale-while-revalidate"
      }
    }
  ]
}